<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Message" table="message">
    <indexes>
      <index name="fk_message_utilisateur1_idx" columns="utilisateur_id_utilisateur"/>
      <index name="fk_message_utilisateur2_idx" columns="utilisateur_id_utilisateur1"/>
    </indexes>
    <id name="idMessage" type="integer" column="id_message">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="contenu" type="string" column="contenu" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="date" type="date" column="date" nullable="true"/>
    <many-to-one field="utilisateurUtilisateur1" target-entity="Utilisateur" fetch="LAZY">
      <join-columns>
        <join-column name="utilisateur_id_utilisateur1" referenced-column-name="id_utilisateur"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="utilisateurUtilisateur" target-entity="Utilisateur" fetch="LAZY">
      <join-columns>
        <join-column name="utilisateur_id_utilisateur" referenced-column-name="id_utilisateur"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
