<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Annonce" table="annonce">
    <indexes>
      <index name="fk_annonce_type_vehicule1_idx" columns="type_vehicule_id_type_vehicule"/>
      <index name="fk_annonce_utilisateur1_idx" columns="utilisateur_id_utilisateur"/>
      <index name="fk_annonce_ville1_idx" columns="ville_id_ville"/>
      <index name="fk_annonce_horaire1_idx" columns="horaire_id_horaire"/>
    </indexes>
    <id name="idAnnonce" type="integer" column="id_annonce">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="titre" type="string" column="titre" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nbPersonnes" type="integer" column="nb_personnes" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="vehicule" type="boolean" column="vehicule" nullable="true"/>
    <field name="dateFixe" type="boolean" column="date_fixe" nullable="true"/>
    <field name="dateLimite" type="datetime" column="date_limite" nullable="true"/>
    <field name="prixTotal" type="integer" column="prix_total" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="telephone" type="integer" column="telephone" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="description" type="text" column="description" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="villeVille" target-entity="Ville" fetch="LAZY">
      <join-columns>
        <join-column name="ville_id_ville" referenced-column-name="id_ville"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="utilisateurUtilisateur" target-entity="Utilisateur" fetch="LAZY">
      <join-columns>
        <join-column name="utilisateur_id_utilisateur" referenced-column-name="id_utilisateur"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="typeVehiculeTypeVehicule" target-entity="TypeVehicule" fetch="LAZY">
      <join-columns>
        <join-column name="type_vehicule_id_type_vehicule" referenced-column-name="id_type_vehicule"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="horaireHoraire" target-entity="Horaire" fetch="LAZY">
      <join-columns>
        <join-column name="horaire_id_horaire" referenced-column-name="id_horaire"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
