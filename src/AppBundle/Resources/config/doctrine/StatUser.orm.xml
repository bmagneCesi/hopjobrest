<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\StatUser" table="stat_user">
    <indexes>
      <index name="fk_stat_user_utilisateur1_idx" columns="utilisateur_id_utilisateur"/>
    </indexes>
    <id name="idStatUser" type="integer" column="id_stat_user">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nbJob" type="integer" column="nb_job" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="revenu" type="float" column="revenu" precision="10" scale="0" nullable="true"/>
    <many-to-one field="utilisateurUtilisateur" target-entity="Utilisateur" fetch="LAZY">
      <join-columns>
        <join-column name="utilisateur_id_utilisateur" referenced-column-name="id_utilisateur"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
